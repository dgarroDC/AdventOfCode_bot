data:

year is text
token is text
leaderboardId is text
leaderboardCode is text
sessionCookie is text

procedure:

store "2021" in year # year of the Advent of Code
store "PASTE_YOUR_BOT_TOKEN_HERE" in token
store "PASTE_THE_LEADERBOR_ID_HERE" in leaderboardId # https://adventofcode.com/{year}/leaderboard/private/view/{leaderboardId}
store "PASTE_THE_LEADERBOR_JOIN_CODE_HERE" in leaderboardCode
store "PASTE_YOUR_SESSION_COOKIE_HERE" in sessionCookie